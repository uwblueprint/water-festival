{
	"env": {
		"node": true,
		"browser": true,
		"es6": true
	},
	"plugins": [
		"react"
	],
	"extends": [
		"airbnb",
		"eslint:recommended"
	],
	"globals": {
		"Expo": false
	},
	"parser": "babel-eslint",
	"rules": {
		"react/jsx-filename-extension": 0,
		"react/prop-types": 2,
		"react/forbid-prop-types": 0,
		"react/jsx-curly-spacing": [2, "always", {
		  "spacing": {"objectLiterals": "never"}
		}],
		"react/jsx-indent": [2, "tab"],
		"react/jsx-indent-props": [2, "tab"]
	}
}
